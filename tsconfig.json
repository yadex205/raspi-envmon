{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "baseUrl": "src",
    "lib": ["esnext"],
    "strict": true,
    "experimentalDecorators": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "allowJs": false,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "skipLibCheck": true
  }
}
